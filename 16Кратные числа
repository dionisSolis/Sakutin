using System;

namespace qwerty
{
    class Program
    {
        static void Main(string[] args)
        {
            Random random = new Random();
            int firstRandomLimit = 1;
            int secondRandomLimit = 28;
            int firstDigitsLimit = 100;
            int secondDigitsLimit = 999;
            int n = random.Next(firstRandomLimit, secondRandomLimit);
            int cntDigitsModN = 0;

            for (int i = 0; i <= secondDigitsLimit; i+=n) 
            {
                if (i >= firstDigitsLimit)
                {
                    cntDigitsModN++;
                }
            }

            Console.WriteLine($"Кол-во чисел в диапазоне [{firstDigitsLimit};{secondDigitsLimit}], кратных {n} - {cntDigitsModN}");
        }
    }
}
