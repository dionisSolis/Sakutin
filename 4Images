using System;

namespace qwerty
{
    class Program
    {
        static void Main(string[] args)
        {
            int imagesAll = 52;
            int imagesInLine = 3;
            int fullLines;
            int imagesNotInLine;

            fullLines = imagesAll / imagesInLine;
            imagesNotInLine = imagesAll % imagesInLine;
            Console.WriteLine($"Полностью заполненных рядов: {fullLines}\nОставишеся картинки: {imagesNotInLine}");
        }
    }
}
