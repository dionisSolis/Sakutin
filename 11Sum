using System;

namespace qwerty
{
    class Program
    {
        static void Main(string[] args)
        {
            Random random = new Random();
            int randomLimit = 100;
            int firstIntToDivision = 3;
            int secondIntToDivision = 5;
            int sumDigitsModDigitToDivision = 0;
            int number = random.Next(randomLimit);

            for (int i = 1; i <= number; i++)
            {
                if (i % firstIntToDivision == 0 || i % secondIntToDivision == 0)
                {
                    sumDigitsModDigitToDivision += i;
                }
            }

            Console.WriteLine($"Сумма чисел, кратных или {firstIntToDivision}, или {secondIntToDivision}, до {number} равна {sumDigitsModDigitToDivision}");
        }
    }
}
