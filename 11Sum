using System;

namespace qwerty
{
    class Program
    {
        static void Main(string[] args)
        {
            Random rand = new Random();
            int firstIntToDivision = 3;
            int secondIntToDivision = 5;
            int sumDigitsModDigitToDivision = 0;
            int number = rand.Next(100);

            for (int i = 1; i <= number; i++) 
            {
                if (i % firstIntToDivision == 0 || i % secondIntToDivision == 0)
                {
                    sumDigitsModDigitToDivision += i;
                }
            }

            Console.WriteLine($"Сумма чисел, кратных или 3, или 5, до {number} равна {sumDigitsModDigitToDivision}");
        }
    }
}
